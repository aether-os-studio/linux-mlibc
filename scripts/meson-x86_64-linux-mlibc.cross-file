[binaries]
c = 'x86_64-linux-mlibc-gcc'
cpp = 'x86_64-linux-mlibc-g++'
ar = 'x86_64-linux-mlibc-ar'
strip = 'x86_64-linux-mlibc-strip'
pkg-config = 'x86_64-pc-linux-gnu-pkg-config'
# sed adds binaries here.

[host_machine]
system = 'linux'
cpu_family = 'x86_64'
cpu = 'x86_64'
endian = 'little'

[properties]
needs_exe_wrapper = true
